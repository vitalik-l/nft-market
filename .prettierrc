{
    "singleQuote": true,
    "trailingComma": "none",
    "tabWidth": 2,
    "printWidth": 120,
    "arrowParens": "always",
    "endOfLine": "auto",
    "bracketSpacing": true,
    "importOrder": [
        "<THIRD_PARTY_MODULES>",
        "^../(.*)",
        "^./(.*)"
    ],
    "importOrderParserPlugins": [
        "typescript",
        "jsx",
        "decorators-legacy"
    ],
    "importOrderSeparation": true,
    "importOrderSortSpecifiers": true
}